{"ast":null,"code":"import request from \"@/utils/request\";\n\n// 管理员模块接口请求统一前缀\nlet baseUrl = \"/admin/\";\nexport default {\n  data() {\n    return {\n      user: {},\n      // colWidth: 150,\n      tableData: [],\n      pageNum: 1,\n      dialogVisible: false,\n      search: {},\n      form: {},\n      total: 0\n    };\n  },\n  // 页面加载的时候，做一些事情，在created里面\n  mounted() {\n    this.load();\n  },\n  // 定义一些页面上控件出发的事件调用的方法\n  methods: {\n    operationSpace() {\n      let width = 0;\n      let _this = this;\n      setTimeout(() => {\n        let rows = document.getElementsByClassName('el-table__row');\n        if (rows && rows.length) {\n          let tds = rows[0].getElementsByTagName('td');\n          let btns = tds[tds.length - 1].getElementsByTagName('button');\n          for (let i = 0; i < btns.length; i++) {\n            width += btns[i].offsetWidth + 5;\n          }\n          _this.colWidth = width;\n        }\n      }, 0);\n    },\n    // 分页加载表格数据\n    load() {\n      request.post(baseUrl + \"page?pageNum=\" + this.pageNum, this.search).then(res => {\n        if (res.code === '0') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n          this.operationSpace();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    // 新增初始化对话框\n    add() {\n      this.form = {};\n      this.$nextTick(() => {\n        this.$refs.tx.clearFiles();\n      });\n      this.dialogVisible = true;\n    },\n    // 编辑初始化模态框\n    edit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.$nextTick(() => {\n        this.$refs.tx.clearFiles();\n      });\n      this.dialogVisible = true;\n    },\n    save() {\n      if (!this.form.id) {\n        // 没有ID，说明是新增操作\n        request.post(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success(\"添加成功\");\n            this.dialogVisible = false;\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        });\n      } else {\n        // 有ID，说明是更新操作\n        request.put(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success('更新成功');\n            this.dialogVisible = false;\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        });\n      }\n    },\n    // 根据ID删除某条数据\n    del(id) {\n      request.delete(baseUrl + id).then(res => {\n        if (res.code === '0') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    txSuccessUpload(res) {\n      this.form.avatar = res.data;\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","baseUrl","data","user","tableData","pageNum","dialogVisible","search","form","total","mounted","load","methods","operationSpace","width","_this","setTimeout","rows","document","getElementsByClassName","length","tds","getElementsByTagName","btns","i","offsetWidth","colWidth","post","then","res","code","list","$notify","error","msg","add","$nextTick","$refs","tx","clearFiles","edit","row","JSON","parse","stringify","save","id","success","put","del","delete","txSuccessUpload","avatar","handleCurrentChange"],"sources":["src/views/manager/Admin.vue"],"sourcesContent":["<!-- 用户管理/管理员页面 -->\n<template>\n  <div>\n    <div>\n      <div class=\"main-body-content\">\n        <!-- 顶部 -->\n        <div class=\"main-body-search\">\n          <div class=\"main-body-header\">管理员</div>\n          <!-- 输入的用户名绑定到search.username上 -->\n          <el-input size=\"small\" class=\"main-input\" placeholder=\"请输入用户名\" v-model=\"search.username\"></el-input>\n          <!-- 点击按钮调用对应的方法 -->\n          <el-button size=\"small\" type=\"primary\" @click=\"load()\">查询</el-button>\n          <el-button size=\"small\" type=\"primary\" @click=\"add()\">新增</el-button>\n        </div>\n\n        <!-- 中部表格 -->\n        <div class=\"main-table-box\">\n          <!-- 表格数据来自tableData -->\n          <el-table style=\"width: 100%\" :data=\"tableData\" size=\"small\" strip header-cell-class-name=\"tableHeaderClass\" >\n            <!-- 用户名列 -->\n\t\t\t\t\t\t<el-table-column label=\"用户名\" prop=\"username\"></el-table-column>\n\n            <!-- 头像列 -->\n\t\t\t\t\t\t<el-table-column label=\"头像\">\n\t\t\t\t\t\t\t<template v-slot=\"scope\">\n                <!-- scope.row是当前行的数据对象，.avatar是头像图片的URL -->\n                <!-- preview-src-list=\"[scope.row.avatar]设置图片预览功能 -->\n\t\t\t\t\t\t\t\t<el-image :src=\"scope.row.avatar\" :preview-src-list=\"[scope.row.avatar]\" style=\"width: 50px; height: 50px; border-radius: 50%\"></el-image>\n\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t</el-table-column>\n\n            <!-- 手机号列 -->\n\t\t\t\t\t\t<el-table-column label=\"手机号\" prop=\"phone\"></el-table-column>\n\n            <!-- 性别列 -->\n\t\t\t\t\t\t<el-table-column label=\"性别\" prop=\"sex\"></el-table-column>\n\n            <!-- 操作列 -->\n            <!-- <el-table-column label=\"操作\" :width=colWidth fixed=\"right\"> -->\n            <el-table-column label=\"操作\" fixed=\"right\">\n              <template v-slot=\"scope\">\n                <!-- scope.row是当前行的数据对象 -->\n                <!-- 点击编辑之后，调用edit方法，并且把当前行的数据对象作为参数传递给edit方法 -->\n\t\t\t\t\t\t\t\t<el-button size=\"small\" type=\"primary\" @click=\"edit(scope.row)\">编辑</el-button>\n\n                <!-- el-popconfirm是一个带有提示内容的弹出框，点击删除按钮的时候通过这个弹出框确认，弹出框自带确认和取消按钮 -->\n                <!-- 确认删除之后，会调用del方法，并且把当前行的id作为参数传递给del方法 -->\n\t\t\t\t\t\t\t\t<el-popconfirm title=\"确定删除？\" @confirm=\"del(scope.row.id)\">\n                  <template #reference>\n                    <el-button size=\"small\" type=\"danger\">删除</el-button>\n                  </template>\n                  <template #confirm-button>\n                    <el-button type=\"primary\" size=\"small\" style=\"background-color: #ff6347; border-color: #ff6347;\">确定</el-button>\n                  </template>\n                  <template #cancel-button>\n                    <el-button type=\"danger\" size=\"small\" style=\"background-color: #f5f5f5; border-color: #dcdcdc; color: #606266;\">取消</el-button>\n                  </template>\n\t\t\t\t\t\t\t\t\t<!-- <el-button size=\"small\" type=\"danger\" slot=\"reference\">删除</el-button> -->\n\t\t\t\t\t\t\t\t</el-popconfirm>\n              </template>\n            </el-table-column>\n          </el-table>\n        </div>\n\n        <!-- 尾部翻页 -->\n        <div style=\"margin: 20px 0\">\n          <el-pagination\n              background\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"pageNum\"\n              :page-size=\"5\"\n              layout=\"total, prev, pager, next\"\n              :total=\"total\">\n          </el-pagination>\n        </div>\n      </div>\n    </div>\n\n    <el-dialog title=\"请填写信息\" :visible.sync=\"dialogVisible\" width=\"40%\">\n      <el-form :model=\"form\" label-position=\"right\" label-width=\"100px\" style=\"padding-right: 40px\">\n        <el-form-item label=\"头像\">\n          <el-upload action=\"http://localhost:8080/files/upload\" ref=\"tx\" :on-success=\"txSuccessUpload\">\n            <el-button size=\"small\" type=\"success\">点击上传</el-button>\n          </el-upload>\n        </el-form-item>\n        <el-form-item label=\"用户名\">\n\t\t\t\t\t<el-input size=\"small\" v-model=\"form.username\" placeholder=\"请输入用户名\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"手机号\">\n\t\t\t\t\t<el-input size=\"small\" v-model=\"form.phone\" placeholder=\"请输入手机号\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"性别\">\n\t\t\t\t\t<el-radio-group size=\"small\" v-model=\"form.sex\">\n\t\t\t\t\t\t<el-radio label=\"男\"></el-radio>\n\t\t\t\t\t\t<el-radio label=\"女\"></el-radio>\n\t\t\t\t\t</el-radio-group>\n\t\t\t\t</el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n          <el-button size=\"small\" @click=\"dialogVisible = false\">取 消</el-button>\n          <el-button size=\"small\" type=\"primary\" @click=\"save\">保 存</el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\n\n// 管理员模块接口请求统一前缀\nlet baseUrl = \"/admin/\"\n\n\nexport default {\n  data() {\n    return {\n      user: {},\n      // colWidth: 150,\n      tableData: [],\n      pageNum: 1,\n      dialogVisible: false,\n      search: {},\n      form: {\n\n      },\n      total: 0,\n\n    }\n  },\n  // 页面加载的时候，做一些事情，在created里面\n  mounted() {\n    this.load()\n  },\n  // 定义一些页面上控件出发的事件调用的方法\n  methods: {\n    operationSpace() {\n      let width = 0\n      let _this = this\n      setTimeout(() => {\n        let rows = document.getElementsByClassName('el-table__row');\n        if (rows && rows.length) {\n          let tds = rows[0].getElementsByTagName('td')\n          let btns = tds[tds.length - 1].getElementsByTagName('button')\n          for (let i = 0; i <btns.length; i++) {\n            width += btns[i].offsetWidth + 5\n          }\n          _this.colWidth = width\n        }\n      }, 0)\n    },\n    // 分页加载表格数据\n    load() {\n\n      request.post(baseUrl + \"page?pageNum=\" + this.pageNum, this.search).then(res => {\n        if (res.code === '0') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n          this.operationSpace();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    // 新增初始化对话框\n    add() {\n      this.form = {};\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.$refs.tx.clearFiles()\n\t\t\t});\n\n      this.dialogVisible = true;\n    },\n    // 编辑初始化模态框\n    edit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.$refs.tx.clearFiles()\n\t\t\t});\n\n      this.dialogVisible = true;\n    },\n\n    save() {\n\n      if (!this.form.id) {\n        // 没有ID，说明是新增操作\n        request.post(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success(\"添加成功\");\n            this.dialogVisible = false;\n\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        })\n      } else {\n        // 有ID，说明是更新操作\n        request.put(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success('更新成功');\n            this.dialogVisible = false;\n\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        })\n      }\n    },\n    // 根据ID删除某条数据\n    del(id) {\n      request.delete(baseUrl + id).then(res => {\n        if (res.code === '0') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n\t\ttxSuccessUpload(res) {\n\t\t\tthis.form.avatar = res.data;\n\t\t},\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n  }\n}\n</script>\n"],"mappings":"AA4GA,OAAAA,OAAA;;AAGA;AACA,IAAAC,OAAA;AAGA;EACAC,KAAA;IACA;MACAC,IAAA;MACA;MACAC,SAAA;MACAC,OAAA;MACAC,aAAA;MACAC,MAAA;MACAC,IAAA,GAEA;MACAC,KAAA;IAEA;EACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA,IAAAC,KAAA;MACA,IAAAC,KAAA;MACAC,UAAA;QACA,IAAAC,IAAA,GAAAC,QAAA,CAAAC,sBAAA;QACA,IAAAF,IAAA,IAAAA,IAAA,CAAAG,MAAA;UACA,IAAAC,GAAA,GAAAJ,IAAA,IAAAK,oBAAA;UACA,IAAAC,IAAA,GAAAF,GAAA,CAAAA,GAAA,CAAAD,MAAA,MAAAE,oBAAA;UACA,SAAAE,CAAA,MAAAA,CAAA,GAAAD,IAAA,CAAAH,MAAA,EAAAI,CAAA;YACAV,KAAA,IAAAS,IAAA,CAAAC,CAAA,EAAAC,WAAA;UACA;UACAV,KAAA,CAAAW,QAAA,GAAAZ,KAAA;QACA;MACA;IACA;IACA;IACAH,KAAA;MAEAX,OAAA,CAAA2B,IAAA,CAAA1B,OAAA,0BAAAI,OAAA,OAAAE,MAAA,EAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA1B,SAAA,GAAAyB,GAAA,CAAA3B,IAAA,CAAA6B,IAAA;UACA,KAAAtB,KAAA,GAAAoB,GAAA,CAAA3B,IAAA,CAAAO,KAAA;UACA,