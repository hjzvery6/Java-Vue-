his.$notify.success('更新成功');\n            this.dialogVisible = false;\n\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        })\n      }\n    },\n    // 根据ID删除某条数据\n    del(id) {\n      request.delete(baseUrl + id).then(res => {\n        if (res.code === '0') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n  }\n}\n</script>\n"],"mappings":"AAiEA,OAAAA,OAAA;;AAEA;AACA,IAAAC,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAAA;MACAC,SAAA;MACAC,OAAA;MACAC,aAAA;MACAC,MAAA;MACAC,IAAA;MACAC,KAAA;IACA;EACA;EAEA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EAEA;EACAC,OAAA;IACA;IACAC,eAAA;MACA,IAAAC,KAAA;MACA,IAAAC,KAAA;MACAC,UAAA;QACA,IAAAC,IAAA,GAAAC,QAAA,CAAAC,sBAAA;QACA,IAAAF,IAAA,IAAAA,IAAA,CAAAG,MAAA;UACA,IAAAC,GAAA,GAAAJ,IAAA,IAAAK,oBAAA;UACA,IAAAC,IAAA,GAAAF,GAAA,CAAAA,GAAA,CAAAD,MAAA,MAAAE,oBAAA;UACA,SAAAE,CAAA,MAAAA,CAAA,GAAAD,IAAA,CAAAH,MAAA,EAAAI,CAAA;YACAV,KAAA,IAAAS,IAAA,CAAAC,CAAA,EAAAC,WAAA;UACA;UACAV,KAAA,CAAAW,QAAA,GAAAZ,KAAA;QACA;MACA;IACA;IAEA;IACAH,KAAA;MACAf,OAAA,CAAA+B,IAAA,CAAA9B,OAAA,0BAAAQ,OAAA,OAAAE,MAAA,EAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA1B,SAAA,GAAAyB,GAAA,CAAA/B,IAAA,CAAAiC,IAAA;UACA,KAAAtB,KAAA,GAAAoB,GAAA,CAAA/B,IAAA,CAAAW,KAAA;UACA,KAAAI,cAAA;QACA;UACA,KAAAmB,OAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IAEA;IACAC,IAAA;MACA,KAAA3B,IAAA;MACA,KAAAF,aAAA;IACA;IAEA8B,KAAA;MACA,KAAA5B,IAAA,CAAA6B,MAAA,QAAAtC,IAAA,CAAAuC,EAAA;MACA,KAAA9B,IAAA,CAAA+B,IAAA,QAAAxC,IAAA,CAAAwC,IAAA;MACA,UAAA/B,IAAA,CAAA8B,EAAA;QACA;QACA1C,OAAA,CAAA+B,IAAA,CAAA9B,OAAA,OAAAW,IAAA,EAAAoB,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAE,OAAA,CAAAQ,OAAA;YACA,KAAAlC,aAAA;YAEA,KAAAK,IAAA;UACA;YACA,KAAAqB,OAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA;QACA;MACA;QACA;Q