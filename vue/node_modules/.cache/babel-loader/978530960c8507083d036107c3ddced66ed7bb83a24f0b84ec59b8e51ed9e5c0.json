-center\">\r\n        <div class=\"header-nav\">\r\n          <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n\t\t\t\t\t\t<el-menu-item index=\"/front/index\">首页</el-menu-item>\r\n\t\t\t\t\t\t<el-menu-item index=\"/front/goods\">所有商品</el-menu-item>\r\n\t\t\t\t\t\t<el-menu-item v-if=\"user.role !== 1\" index=\"/front/cart\">我的购物车</el-menu-item>\r\n\t\t\t\t\t\t<el-menu-item v-if=\"user.role !== 1\" index=\"/front/order\">我的订单</el-menu-item>\r\n          </el-menu>\r\n        </div>\r\n      </div>\r\n      <div style=\"width: 300px\">\r\n        <el-input size=\"medium\" style=\"width: 200px\" v-model=\"name\" clearable placeholder=\"请输入商品名称搜索\"></el-input>\r\n        <el-button size=\"medium\" @click=\"search\" style=\"margin-left: 5px\">搜索</el-button>\r\n      </div>\r\n      <div class=\"header-right\">\r\n        <div v-if=\"!user.username\">\r\n          <el-button @click=\"$router.push('/login')\">登录</el-button>\r\n          <el-button @click=\"$router.push('/register')\">注册</el-button>\r\n        </div>\r\n        <div v-else>\r\n          <el-dropdown>\r\n            <div class=\"header-dropdown\">\r\n              <img :src=\"user.avatar\" alt=\"\">\r\n              <div style=\"margin-left: 10px\">\r\n                <span>{{ user.username }}</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\r\n              </div>\r\n            </div>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item>\r\n                <div @click=\"goToPerson()\">个人中心</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"logout\">退出</div>\r\n              </el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!--主体-->\r\n    <div class=\"main-body\">\r\n      <router-view ref=\"child\" />\r\n    </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"FrontLayout\",\r\n\r\n  data () {\r\n    return {\r\n      top: '',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      name: ''\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    request.get('/notice').then(res => {\r\n      this.notice = res.data\r\n      let i = 0\r\n      if (this.notice && this.notice.length) {\r\n        this.top = this.notice[0].content\r\n        setInterval(() => {\r\n          this.top = this.notice[i].content\r\n          i++\r\n          if (i === this.notice.length) {\r\n            i = 0\r\n          }\r\n        }, 2500)\r\n      }\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    goToPerson() {\r\n\t\t\tif (this.user.role === 1) {\r\n\t\t\t\tthis.$router.push(\"/end/pAdmin\")\r\n\t\t\t}\r\n\t\t\tif (this.user.role === 2) {\r\n\t\t\t\tthis.$router.push(\"/end/pUser\")\r\n\t\t\t}\r\n\r\n    },\r\n    // 退出登录\r\n    logout() {\r\n      localStorage.removeItem(\"user\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n    search() {\r\n      this.$router.replace('/front/search?name=' + this.name + '&random' + Math.random())\r\n      this.$refs.child.searchGoods()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  @import \"@/assets/css/front.css\";\r\n</style>"],"mappings":";AA0DA,OAAAA,OAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,GAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAL,IAAA;IACA;EACA;EAEAQ,QAAA;IACAT,OAAA,CAAAU,GAAA,YAAAC,IAAA,CAAAC,GAAA;MACA,KAAAC,MAAA,GAAAD,GAAA,CAAAV,IAAA;MACA,IAAAY,CAAA;MACA,SAAAD,MAAA,SAAAA,MAAA,CAAAE,MAAA;QACA,KAAAZ,GAAA,QAAAU,MAAA,IAAAG,OAAA;QACAC,WAAA;UACA,KAAAd,GAAA,QAAAU,MAAA,CAAAC,CAAA,EAAAE,OAAA;UACAF,CAAA;UACA,IAAAA,CAAA,UAAAD,MAAA,CAAAE,MAAA;YACAD,CAAA;UACA;QACA;MACA;IACA;EACA;EAEAI,OAAA;IACAC,WAAA;MACA,SAAAf,IAAA,CAAAgB,IAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA;MACA,SAAAlB,IAAA,CAAAgB,IAAA;QACA,KAAAC,OAAA,CAA